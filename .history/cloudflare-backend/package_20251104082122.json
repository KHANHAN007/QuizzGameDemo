{
  "name": "quiz-game-cloudflare-backend",
  "version": "2.0.0",
  "description": "Quiz Game backend with Authentication - Cloudflare Workers + D1",
  "main": "src/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "tail": "wrangler tail",
    "db:create": "wrangler d1 create quiz-game-db",
    "db:init": "wrangler d1 execute quiz-game-db --local --file=./schema.sql",
    "db:init:remote": "wrangler d1 execute quiz-game-db --remote --file=./schema.sql",
    "db:seed": "wrangler d1 execute quiz-game-db --local --file=./seed.sql",
    "db:seed:remote": "wrangler d1 execute quiz-game-db --remote --file=./seed.sql",
    "db:seed-users": "wrangler d1 execute quiz-game-db --local --file=./seed-users.sql",
    "db:seed-users:remote": "wrangler d1 execute quiz-game-db --remote --file=./seed-users.sql",
    "db:setup": "npm run db:init && npm run db:seed-users && npm run db:seed",
    "db:setup:remote": "npm run db:init:remote && npm run db:seed-users:remote && npm run db:seed:remote",
    "hash": "node hash-password.js"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "d1",
    "quiz",
    "authentication"
  ],
  "author": "KHANHAN007",
  "license": "MIT",
  "devDependencies": {
    "wrangler": "^4.45.3"
  }
}
